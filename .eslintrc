{  
    "extends": "airbnb",  
    "parser": "babel-eslint",  
    "env": 
    {    
        "node": true,    
        "es6": true,    
        "browser": true,
        "jest/globals": true
    },  
    "rules": 
    {    
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],    
        "implicit-arrow-linebreak": "off",    
        "comma-dangle": "off",    
        "indent": "off",    
        "no-trailing-spaces": "off",
        "no-console": 0
    },
    "settings": {
        "import/resolver": {
            "webpack": {
                "config": "webpack.config.js"
            }
        }
    }
}